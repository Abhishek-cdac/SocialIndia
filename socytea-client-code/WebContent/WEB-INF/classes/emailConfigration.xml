<struts>
 <package name="emailconfig" extends="default">

 <action name="emailconfupdate" class="com.socialindia.emailconfig.Emailconfigmgmt" method="emailUpdate">
 <result name="success">resources/jsp/emailconfig/ajax.jsp</result>
    <result name="input">resources/jsp/emailconfig/ajax.jsp</result>
  </action>
   <action name="emailEnginStart" class="com.socialindia.emailconfig.Emailconfigmgmt" method="emailEnginStart">
 <result name="success">resources/jsp/emailconfig/ajax.jsp</result>
